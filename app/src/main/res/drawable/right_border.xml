<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="rectangle">
            <stroke
                android:width="5dp"
                android:color="@color/transparent" />
            <solid
                android:color="@color/white" />
        </shape>
    </item>
    <item
        android:right="5dp">
        <shape
            android:shape="rectangle">
            <stroke
                android:color="@color/white" />
            <solid
                android:color="@color/white" />
        </shape>
    </item>
</layer-list>